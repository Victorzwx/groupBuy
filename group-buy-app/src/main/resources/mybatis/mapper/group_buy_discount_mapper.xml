<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bugstack.infrastructure.dao.IGroupBuyDiscountDao">


    <select id="queryGroupBuyDiscountList" resultType="cn.bugstack.infrastructure.dao.po.GroupBuyDiscount">
        select * from group_buy_discount
    </select>
    <select id="queryGroupBuyActivityDiscountByDiscountId"
            resultType="cn.bugstack.infrastructure.dao.po.GroupBuyDiscount">
        select discount_id, discount_name, discount_desc, discount_type, market_plan, market_expr, tag_id
        from group_buy_discount
        where discount_id = #{discountId}
    </select>
</mapper>
